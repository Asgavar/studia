namespace ContextFreeGrammars
{
    public class NonTerminalSymbol : ISymbol
    {
        private string repr;


        public NonTerminalSymbol(string repr)
        {
            this.repr = repr;
        }
        

        public string GetRepresentation()
        {
            return repr;
        }


        public bool IsTerminal()
        {
            return false;
        }
    }
}
